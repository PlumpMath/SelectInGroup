Call Main()
Sub Main()
	Dim arrObjects
	
	arrObjects = Rhino.GetObjects("Select Objects to Select", , False)
	If Not IsNull(arrObjects) Then
		Rhino.SelectObjects(arrObjects)
	Else
		Exit Sub
	End If
End Sub